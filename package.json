	"scripts": {
		"clean": "rm -rf public/assets/*",
		"test": "bun test",
		"type-check": "tsc --noEmit",
		"lint": "biome check ./resources/scripts",
		"format": "biome format --write ./resources/scripts",
		"build:dev": "bun run clean && bun run build.ts",
		"build:dev-hash": "bun run clean && bun run build.ts --hash",
		"build:prod": "bun run clean && bun run build.ts --production",
		"build:prod-compress": "bun run clean && bun run build.ts --production --compress",
		"build:clean:prod": "rm -rf node_modules && bun install && bun run build:prod",
		"package:dev": "bun run clean && bun run build.ts --archive",
		"package:dev-hash": "bun run clean && bun run build.ts --hash --archive",
		"package:dev-hash-compress": "bun run clean && bun run build.ts --hash --compress --archive",
		"package:dev-hash-split": "bun run clean && bun run build.ts --hash --split --archive",
		"package:dev-hash-split-compress": "bun run clean && bun run build.ts --hash --split --compress --archive",
		"package:prod": "bun run clean && bun run build.ts --production --archive",
		"package:prod-compress": "bun run clean && bun run build.ts --production --compress --archive",
		"package:prod-split": "bun run clean && bun run build.ts --production --split --archive",
		"package:prod-compress-split": "bun run clean && bun run build.ts --production --compress --split --archive",
		"watch": "bun run build.ts --watch",
		"serve": "bun run build.ts --watch",
		"prepare": "husky"
	},
	"browserslist": [
		"> 0.5%",
		"last 2 versions",
		"firefox esr",
		"not dead"
	],
	"babelMacros": {
		"twin": {
			"preset": "styled-components"
		},
		"styledComponents": {
			"pure": true,
			"displayName": true,
			"fileName": true
		}
	}
}
