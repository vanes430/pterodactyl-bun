	"scripts": {
		"clean": "rm -rf public/assets/*",
		"test": "bun test",
		"type-check": "tsc --noEmit",
		"lint": "biome check ./resources/scripts",
		"format": "biome format --write ./resources/scripts",
		"build:dev": "bun run clean && NODE_ENV=development bun run build.ts --minify=false",
		"build:dev-hash": "bun run clean && NODE_ENV=production bun run build.ts --minify=false",
		"build:prod": "bun run clean && NODE_ENV=production bun run build.ts",
		"package:dev": "MODE=dev bash package-panel.sh",
		"package:dev-hash": "MODE=dev-hash bash package-panel.sh",
		"package:prod": "MODE=prod bash package-panel.sh",
		"watch": "NODE_ENV=development bun run build.ts --watch",
		"serve": "NODE_ENV=development bun run build.ts --watch",
		"prepare": "husky"
	},
	"browserslist": [
		"> 0.5%",
		"last 2 versions",
		"firefox esr",
		"not dead"
	],
	"babelMacros": {
		"twin": {
			"preset": "styled-components"
		},
		"styledComponents": {
			"pure": true,
			"displayName": true,
			"fileName": true
		}
	}
}
